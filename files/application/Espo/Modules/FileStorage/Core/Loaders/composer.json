{
    "name": "espocrm/file-storage",
    "type": "library",
    "version": "0.0.1",
    "description": "File Storage Services for EspoCRM",
    "keywords": ["filesystem", "espocrm", "storage"],
    "homepage": "https://github.com/theBuzzyCoder/espocrm-file-storage-services",
    "readme": "https://github.com/theBuzzyCoder/espocrm-file-storage-services/blob/master/README.md",
    "time": "2020-02-06",
    "authors": [
        {
            "name": "Karthik Bhat K",
            "email": "theBuzzyCoder@gmail.com",
            "homepage": "https://linkedin.com/in/theBuzzyCoder",
            "role": "Senior Engineer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-apcu": "*",
        "league/flysystem": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "license": "proprietary",
    "support": {
        "email": "theBuzzyCoder@gmail.com",
        "issues": "https://github.com/theBuzzyCoder/espocrm-file-storage-services/issues",
        "source": "https://github.com/theBuzzyCoder/espocrm-file-storage-services"
    },
    "scripts": {
        "test": [
            "@composer install",
            "./Providers/packages/bin/phpunit -c phpunit.xml"
        ]
    },
    "scripts-descriptions": {
        "test": "Run all tests!"
    },
    "config": {
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "sort-packages": true
    }
}
